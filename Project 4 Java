
public class Project4 {

	public static void main(String[] args) {
		int arr[][]={{1,3,4},{2,4,3},{3,4,5}};
		int arr1[][]={{1,3,4},{2,4,3},{1,2,4}};
		for(int i=0;i<3;i++){
			for(int j=0;j<3;j++){
				System.out.print(arr[i][j]+arr1[i][j]+" "); //for multiplication we just replace the addition "+" sign with multiplication i.e. "*" between "arr[i][j]" and "arr1[i][j]"
			}
			System.out.println();
		}

	}

}
